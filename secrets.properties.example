# 复制此文件为 secrets.properties 并填入你的实际 API 密钥
# secrets.defaults.properties 文件会被 .gitignore 忽略，不会提交到版本控制

# DeepSeek API Key
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Custom Provider Configuration
CUSTOM_PROVIDER_API_BASE_URL=https://your-custom-provider.com/v1/
CUSTOM_PROVIDER_API_KEY=your_custom_provider_api_key_here

# Azure API 配置
azureApiKey=your_azure_api_key_here
azureRegion=your_azure_region_here
testAiHubMixToken=your_test_token_here